---
import CSS from "./PageList.module.scss"

import Card from "@components/Card/Card.astro";

const { pages } = Astro.props;
---

{
    pages.map((page: { slug: string; data: any; }) => {
        const { slug, data } = page;
        return <Card
            title={ data.title }
            description={ data.description }
            url={ slug }
            />
    })
}
