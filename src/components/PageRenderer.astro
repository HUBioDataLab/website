---
import MarkdownLayout from '@layouts/MarkdownLayout.astro';

export interface Props {
    pagePage: any,
};

const { pagePage } = Astro.props as Props;
const { data } = pagePage;
---

<MarkdownLayout page={ pagePage }>
    <h1>{ data.title }</h1>

    {
        data.image && <img src={ data.image } alt="" width="25%">
    }
</MarkdownLayout>